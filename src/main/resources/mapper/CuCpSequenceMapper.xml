<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nbl.dao.CuCpSequenceDao">
	<resultMap id="BaseResultMap" type="com.nbl.model.CuCpSequenceKey">
		<id column="ID" property="id" jdbcType="INTEGER" />
		<id column="CURDATE" property="curdate" jdbcType="CHAR" />
	</resultMap>
	<delete id="deleteByPrimaryKey" parameterType="com.nbl.model.CuCpSequenceKey">
		delete from CU_CP_SEQUENCE
		where ID = #{id,jdbcType=INTEGER}
		and CURDATE = #{curdate,jdbcType=CHAR}
	</delete>
	<insert id="insert" parameterType="com.nbl.model.CuCpSequenceKey">
		<selectKey resultType="java.lang.Integer" order="AFTER"
			keyProperty="id">
			SELECT LAST_INSERT_ID() AS ID
		</selectKey>
		insert into CU_CP_SEQUENCE (ID, CURDATE)
		values (#{id,jdbcType=INTEGER}, #{curdate,jdbcType=CHAR})
	</insert>
	<insert id="insertSelective" parameterType="com.nbl.model.CuCpSequenceKey">
		insert into CU_CP_SEQUENCE
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="id != null">
				ID,
			</if>
			<if test="curdate != null">
				CURDATE,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="id != null">
				#{id,jdbcType=INTEGER},
			</if>
			<if test="curdate != null">
				#{curdate,jdbcType=CHAR},
			</if>
		</trim>
	</insert>
</mapper>